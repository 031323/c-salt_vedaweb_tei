                  <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
               </data>
               <zeroOrMore>
                  <data type="token">
                     <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
                  </data>
               </zeroOrMore>
            </list>
         </attribute>
      </optional>
   </define>
